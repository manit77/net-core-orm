@using CoreORM
@using CoreUtils
@model DBDatabase
using JMS.Data.ORM;
namespace JMS.Data;

public static class Schemas
{
@foreach (var table in Model.Tables)
{
    string tablename = table.MappedName;
@:    public static @(tablename)Schema @(tablename)Schema { get; } = new @(tablename)Schema();
}
}
