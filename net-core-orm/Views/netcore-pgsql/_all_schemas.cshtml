@using CoreORM
@using CoreUtils
@model ViewsModel;
using JMS.ORM.Schema;

//Code generated using net-core-orm
namespace JMS.ORM;

public static class Schemas
{
@{
    var tables = Model.Tables;
}    
@foreach (var table in tables)
{
    string tablename = table.MappedName;
@:    public static @(tablename)Schema @(tablename)Schema { get; } = new @(tablename)Schema();
}
@{
@:    public static List@(Html.Raw("<"))IORMSchema> AllSchemas =>
@:    [
       @foreach (var table in tables)
        {
            string tablename = table.MappedName;
@:            @(tablename)Schema,
        }

@:    ];
}
}
